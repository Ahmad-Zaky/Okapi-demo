{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "ModuleDescriptor",
  "type": "object",
  "properties": {
    "id": {"type": "string"},
    "name": {"type": "string"},
    "url": {"type": "string"},
    "descriptor" : {
      "type": "object",
      "properties": {
        "cmdlineStart": {"type": "string"},
        "cmdlineStop": {"type": "string"}
      }
    },
    "routingEntries": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "methods": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "path": {"type": "string"},
          "level": {"type": "string"},
          "type": {"type": "string"}
        }
      }
    }
  }
}
